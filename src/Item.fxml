<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>

<HBox alignment="CENTER_LEFT" onMouseDragOver="#hover" onMouseEntered="#hover" onMouseExited="#hoverExit" styleClass="root" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ItemController">
    <children>
        <CheckBox fx:id="box" mnemonicParsing="false" onAction="#clickCheck" styleClass="checkbox" />
        <TextField fx:id="TextField" editable="false" maxWidth="1.7976931348623157E308" onKeyPressed="#pressEnter" onMouseClicked="#doublepress" prefHeight="39.0" prefWidth="457.0" promptText="Neuer Eintrag" styleClass="textfield" HBox.hgrow="ALWAYS" />
        <Label fx:id="datum" styleClass="babel" text="Datum" />
        <Button fx:id="loeschen" mnemonicParsing="false" onAction="#buttonClick" opacity="0.0" text="X" />
    </children>
</HBox>
